<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="UI version" content="<%= htmlWebpackPlugin.options.version %>">
    <meta name="viewport"
        content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" />
    <link href="/img/favicon.ico" rel="shortcut icon" type="image/x-icon">
    <% for (key in htmlWebpackPlugin.files.css) { %>
        <link href="<%= htmlWebpackPlugin.files.css[key] %>" rel="stylesheet" />
    <% } %>
    <title>Scenario analysis - Blue Yonder</title>
</head>
<body>
<div id="timer_wrapper">
    <h2 id="timer_title" style="text-align: center">Timer</h2>
    <h4 id="timer_time" style="text-align: center">00:00:00</h4>
    <h5>Lap list</h5>
    <ul id="timer_loop_wrapper">

    </ul>
    <div id="timer_buttons">
        <button id="timer_reset" style="width: 100px">Reset</button>
        <button id="timer_action" style="width: 100px">Stop</button>
    </div>
</div>
<% for (key in htmlWebpackPlugin.files.js) { %>
    <script src="<%= htmlWebpackPlugin.files.js[key] %>"></script>
<% } %>
<style>
    body {
        min-height: 100vh;
        display: flex;
        align-items: center;
        justify-content: center;
    }
</style>
</body>
</html>
